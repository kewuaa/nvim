{
    "cdef/extern": {
        "prefix": "sextern",
        "body": [
            "cdef extern from \"${1:headfile}\" nogil:",
            "\t${2:pass}",
            "#endextern"
        ],
        "description": "Code snippet for the cdef extern statement."
    },
    "cdef/function": {
        "prefix": "scfunc",
        "body": [
            "cdef ${1:type} ${2:name}($3):",
            "\t${4:pass}",
            "#endcdef"
        ],
        "description": "Code snippet for the cdef statement."
    },
    "cpdef": {
        "prefix": "scpfunc",
        "body": [
            "cpdef ${1:type} ${2:name}($3):",
